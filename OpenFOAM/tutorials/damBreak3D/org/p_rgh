FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            fixedFluxPressure;
        phi             phiAbs;
        value           uniform 0;
    }

    obstacle
    {
        type            fixedFluxPressure;
        phi             phiAbs;
        value           uniform 0;
    }

    atmosphere
    {   
        type           fixedValue;
        value          uniform 0;
        //type            totalPressure;
        //p0              uniform 0;
    }
}

